  "dependencies": {
    "axios": "^0.17.1",
    "bulma": "^0.6.1",
    "bulma-react": "0.0.8",
    "chart.js": "^2.7.1",
    "moment": "^2.19.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "ramda": "^0.25.0",
    "rc-select": "^7.1.0",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-burger-menu": "^2.1.11",
    "react-chartjs-2": "^2.6.4",
    "react-datepicker": "^0.60.2",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-modal": "^3.1.5",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "recharts": "^1.0.0-beta.1",
    "redux": "^3.7.2",
    "redux-burger-menu": "^0.2.2",
    "redux-form": "^7.2.0",
    "redux-logger": "^3.0.6",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.75.1"
  },
  "devDependencies": {
    "babel-plugin-react-intl": "^2.3.1",
    "prettier": "1.3.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "REACT_APP_API_URL=http://sometest.com npm-run-all -p watch-css start-js",
    "start-staging": "REACT_APP_API_URL=http://sometest.com npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"src/**/*.{js,jsx}\""
  }
}
